#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,sum=0;
    scanf("%d\n",&n);
    int *arr=(int*)malloc(n*sizeof(int));
    for(int i=0;i<n;i++)
    {
    scanf("%d",&arr[i]);
    }
    for(int j=n-1;j>=0;j--)
    {
        printf("%d",arr[j]);
    }
   
    free(arr);
     
    return 0;
}
